# include <stdio.h>
# include <stdbool.h>

void menu(void){
    printf("\n1. Insert\n2. Delete\n3. Search\n4. Count the elements in the array\n5. Exit\n Select your choice: ");
}

int main(void) {
    int array[10];
    int choice;

    while (true){
        menu();
        scanf("%d", &choice);
    
        switch (choice) {
            case 1: printf("\n(10,20,30)");
                   break;
            case 2: printf("\n20");
                   break;
            case 3: printf("\n10");
                   break;
            case 4: printf("\n2");
                break;
        case 5: return 0;
        default: printf("Wrong option. Try again");
      }
    }
}




