#include <stdio.h>

int main() {
   
    int c[5] = {1, 2, 3, 4, 5};
    printf("Array elements are:\n");
    for (int i = 0; i < 5; i++) {
        printf("c[%d] = %d\n", i, c[i]);
    }
 c[2] = 10;
    printf("\nModified array elements:\n");
    for (int i = 0; i < 5; i++) {
        printf("c[%d] = %d\n", i, c[i]);
    }
  return 0;
  }

output:
Array elements are:
c[0] = 1
c[1] = 2
c[2] = 3
c[3] = 4
c[4] = 5

Modified array elements:
c[0] = 1
c[1] = 2
c[2] = 10
c[3] = 4
c[4] = 5
